<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp"> </corners>
            <stroke android:width="2dp" android:color="@color/DarkBlue"> </stroke>
            <solid android:color="@color/LPink"> </solid>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="10dp"> </corners>
            <stroke android:width="2dp" android:color="@color/LightGreen"> </stroke>
            <solid android:color="@color/LOrange"> </solid>
        </shape>
    </item>
</selector>